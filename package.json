{
  "name": "react-express-starter",
  "description": "starter kit for react and express",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "concurrently": "^4.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "yarn && concurrently \"yarn run server\" \"yarn run build && yarn run client\"",
    "dev": "concurrently \"yarn run server:dev\" \"yarn run client\"",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "cd server && yarn start",
    "server:dev": "cd server && yarn start"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
